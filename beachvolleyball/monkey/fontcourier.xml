<root
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="Schema/fontspliter.xsd"
      >
  <source>
    <spaceWidth>22</spaceWidth>
    <newLine>26</newLine>
    <image>
      <filePath>fontcourier\fontcourier.tga</filePath>
      <charSequence>abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ01234567890`~!@#$%^*()_+-=[]{};':",./?\|</charSequence>
      <baseLine>44</baseLine>
    </image>
    <image>
      <filePath>fontcourier\fontcourierextra.tga</filePath>
      <charSequence>&#169;&#60;&#62;&#38;</charSequence>
      <baseLine>44</baseLine>
    </image>
  </source>

  <destination>
    <destinationPath>fontcouriersplit\</destinationPath>
    <tagetTexture>font.tga</tagetTexture>
    <targetWidth>2048</targetWidth>
    <targetHeight>2048</targetHeight>
    <writeOriginX>512</writeOriginX>
    <writeOriginY>768</writeOriginY>
    <writeWidth>512</writeWidth>
    <writeHeight>256</writeHeight>
  </destination>
</root>
